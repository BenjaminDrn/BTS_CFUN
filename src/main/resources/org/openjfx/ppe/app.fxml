<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="450" maxWidth="400" minHeight="350" minWidth="300" prefHeight="350.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.ppe.Entree">
   <children>
      <AnchorPane fx:id="Anchor_login" maxHeight="450.0" maxWidth="400.0" minHeight="350.0" minWidth="300.0" prefHeight="350.0" prefWidth="300.0" style="-fx-background-color: white;" visible="true">
         <children>
            <Button fx:id="button_login" layoutX="160.0" layoutY="257.0" mnemonicParsing="false" onAction="#enterComplex" prefHeight="37.0" prefWidth="100.0" text="Entrer">
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </Button>
            <TextField fx:id="fields_lastname" layoutX="45.0" layoutY="202.0" prefHeight="35.0" prefWidth="215.0" promptText="Nom" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="43.0" layoutY="53.0" prefHeight="55.0" prefWidth="149.0" text="BIENVENUE" textAlignment="CENTER">
               <font>
                  <Font name="Arial Bold" size="25.0" />
               </font>
            </Label>
            <Label layoutX="45.0" layoutY="95.0" prefHeight="17.0" prefWidth="212.0" text="Complexe ouvert de 6h à 22h" />
