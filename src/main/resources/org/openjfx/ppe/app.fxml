            <Label layoutX="110.0" layoutY="33.0" prefHeight="17.0" prefWidth="77.0" text="Musculation" />
            <Button disable="false" layoutX="117.0" layoutY="104.0" mnemonicParsing="false" text="Banc" />
            <Button disable="false" layoutX="114.0" layoutY="139.0" mnemonicParsing="false" text="rameur" />
            <Button disable="false" layoutX="100.0" layoutY="170.0" mnemonicParsing="false" text="sac de frappe" />
