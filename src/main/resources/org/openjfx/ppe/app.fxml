      </AnchorPane>
      <AnchorPane fx:id="Anchor_equipementFitness" prefHeight="350.0" prefWidth="300.0" style="-fx-background-color: white;" visible="false">
         <children>
            <Label layoutX="123.0" layoutY="38.0" text="FITNESS" />
            <Button disable="false" layoutX="121.0" layoutY="77.0" mnemonicParsing="false" text="vélo" />
            <Button disable="false" layoutX="96.0" layoutY="117.0" mnemonicParsing="false" text="vélo elliptique" />
            <Button disable="false" layoutX="107.0" layoutY="155.0" mnemonicParsing="false" text="exerciseur" />
            <Button disable="false" layoutX="92.0" layoutY="194.0" mnemonicParsing="false" text="Tapis de course" />
            <Button fx:id="button_backFitnessComplex" layoutX="126.0" layoutY="283.0" mnemonicParsing="false" onAction="#backChooseComplexFitness" text="retour" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
