            <Button fx:id="button_exit" layoutX="45.0" layoutY="257.0" mnemonicParsing="false" onAction="#enterCodebarres" prefHeight="37.0" prefWidth="100.0" text="Sortir">
               <padding>
                  <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
               </padding>
            </Button>
            <Label fx:id="label_errorLogin" layoutX="45.0" layoutY="130.0" prefHeight="13.0" prefWidth="215.0" textFill="#f24e4e">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="Anchor_codebarres" prefHeight="350.0" prefWidth="300.0" style="-fx-background-color: white;" visible="false">
         <children>
            <Label layoutX="72.0" layoutY="86.0" prefHeight="17.0" prefWidth="156.0" text="Sortir du complexe">
               <font>
                  <Font name="Arial Bold" size="16.0" />
               </font>
            </Label>
            <Label layoutX="72.0" layoutY="106.0" prefHeight="17.0" prefWidth="149.0" text="Entrez le numéro du code-barres">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <Button fx:id="button_validExit" layoutX="68.0" layoutY="195.0" mnemonicParsing="false" onAction="#validExitComplex" prefHeight="26.0" prefWidth="160.0" text="Valider" />
            <TextField fx:id="fields_codebarres" layoutX="69.0" layoutY="149.0" prefWidth="160.0" promptText="Ex : 12345678" />
            <Label fx:id="label_errorCodebarres" layoutX="69.0" layoutY="133.0" prefHeight="13.0" prefWidth="160.0" textFill="#f24e4e">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <Button fx:id="button_backCodeBarre" layoutX="121.0" layoutY="243.0" mnemonicParsing="false" onAction="#exitCodebarres" text="retour" />
         </children>
      </AnchorPane>
      <AnchorPane fx:id="Anchor_choixSalle" prefHeight="350.0" prefWidth="300.0" style="-fx-background-color: white;" visible="false">
