<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="350.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.ppe.PrimaryController">
   <children>
      <Label layoutX="25.0" text="Salle de sport C-FUN" textAlignment="CENTER">
         <font>
            <Font name="Arial" size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin></Label>
      <TextField prefHeight="30.0" prefWidth="200.0" promptText="Login" VBox.vgrow="NEVER">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </TextField>
      <PasswordField prefHeight="30.0" prefWidth="200.0" promptText="Mot de passe">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
      </PasswordField>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" prefWidth="100.0" style="-fx-border-radius: 20;" text="Submit" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
